#!/bin/tcsh

foreach problem (`seq 81 82`)

echo "------------------"
echo "problem number" $problem

foreach alg (`seq 0 4`)

echo "algorithm" $alg

foreach weight ( 1.25 1.50 2.0 3.0 4.0 9.0)

echo "with weight" $weight

./bin/release/DSDWA -stp $problem $alg $weight >> results81.txt

end

end

end

mv results81.txt ./papers/DSDWA

python3 ./papers/DSDWA/makeTable.py 5 5
