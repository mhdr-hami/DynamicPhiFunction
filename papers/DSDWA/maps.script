#!/bin/tcsh

foreach alg (`seq 0 8`)

#foreach weight ( 1.25 1.5 2 3 5 9 )
foreach weight (8.0 9.0) #LastDelta

foreach scen (./scenarios/random/*-10-*.scen)

set x = `echo $scen | sed 's/.*\///g'`
set map = `echo $x | sed 's/.scen//'`
echo "ALG " $alg " weight " $weight " map " $map

./bin/release/DSDWA -map ./maps/radnom/$map $scen $alg $weight >> random-results.txt

end
end
end
